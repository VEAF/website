{% extends 'admin/layout.html.twig' %}
{% set libKilikTable=true %}
{% set section="dcs" %}
{% set subSection="player" %}

{% block pageContent %}
    <div class="row mb-4">
        <div class="col-xl-8">
            {% embed "admin/dcs/player/_list.html.twig" with {"table": table} %}
                {% block tableTitle %}
                    <b>Joueurs DCS</b>
                    <div class="float-right">
                        <a href="{{ path('admin_dcs_player_autolink') }}" class="btn btn-primary">autolink de {{ autolink }} joueurs Perun / SLMOD</a>
                    </div>
                {% endblock tableTitle %}
            {% endembed %}
        </div>
    </div>
{% endblock pageContent %}
