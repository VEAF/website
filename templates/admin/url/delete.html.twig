{% extends 'admin/layout.html.twig' %}
{% set libKilikTable=true %}
{% set section="admin" %}
{% set subSection="url" %}

{% block pageContent %}
    <div class="row mb-4">
        <div class="col-md-6">
            {% embed "admin/_common/_delete_dialog.html.twig" with {'title': "Supprimer l'url", 'cancelUrl': path('admin_url_view', {'url': url.id})} %}
                {% block message %}
                    Confirmez-vous la suppression de l'url {{ url.slug }}?
                {% endblock message %}
            {% endembed %}
        </div>
    </div>
{% endblock pageContent %}
